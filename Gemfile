source "https://rubygems.org"

gem "jekyll", "~> 4.2.0"
# gem "jekyll", github: "jekyll/jekyll", ref: "refs/pull/9248/head"

# Windows and JRuby does not include zoneinfo files, so bundle the tzinfo-data gem and associated library.
platforms :mingw, :x64_mingw, :mswin, :jruby do
  gem "tzinfo", "~> 1.2"
  gem "tzinfo-data"
end

gem "webrick", "~> 1.7"
gem "liquid", "~> 4.0.4"
gem 'html-proofer'
gem 'jekyll-gist'
gem 'rouge'
